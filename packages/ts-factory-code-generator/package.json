{
  "name": "@openapi-qraft/ts-factory-code-generator",
  "version": "1.0.0",
  "private": true,
  "type": "commonjs",
  "scripts": {
    "build": "yarn exec ./generateFactoryCodeGenerator.mjs && tsc --build"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": "./dist/index.js",
