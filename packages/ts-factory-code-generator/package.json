  "dependencies": {
    "ts-factory-code-generator-generator": "^0.7.0",
    "typescript-5.5.4": "npm:typescript@5.5.4"
  },
  "devDependencies": {
    "@types/node": "^22.5.5",
    "tsx": "^4.19.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/OpenAPI-Qraft/openapi-qraft.git",
    "directory": "packages/cli"
  }
}
