{
  "name": "@openapi-qraft/plugin",
  "version": "2.0.0-next.4",
  "packageManager": "yarn@4.0.2",
  "type": "module",
  "scripts": {
    "build": "tsc --project tsconfig.build.json",
    "dev": "yarn build --watch --noEmitOnError false",
    "test": "vitest run --coverage",
    "typecheck": "tsc --noEmit",
    "lint": "eslint",
    "clean": "rimraf dist/",
    "write-package-version-file": "yarn exec ../../write-package-version-file.sh"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": "./dist/index.js",
    "./*": "./dist/*.js"
  },
  "typesVersions": {
    "*": {
      "*": [
        "dist/*"
      ]
    }
  },
  "dependencies": {
    "ansi-colors": "^4.1.3",
    "camelcase": "^8.0.0",
    "commander": "^11.1.0",
    "micromatch": "^4.0.8",
    "openapi-typescript": "^7.3.0",
    "ora": "^8.0.1",
    "yaml": "^2.3.4"
  },
  "devDependencies": {
    "@openapi-qraft/test-fixtures": "workspace:*",
    "@types/micromatch": "^4",
    "@types/node": "^18.19.4",
    "commander": "^11.1.0",
    "eslint": "^9.0.0",
    "rimraf": "^5.0.10",
    "typescript": "^5.5.4",
    "vitest": "^1.4.0"
  },
  "files": [
    "dist",
    "src",
    "!dist/**/*.test.*",
    "!dist/**/*.spec.*",
    "!src/**/__snapshots__/**",
    "!src/**/*.test.*",
    "!src/**/*.spec.*"
  ],
  "publishConfig": {
    "access": "public"
  }
}
