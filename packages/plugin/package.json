{
  "name": "@qraft/plugin",
  "version": "1.0.0-beta.2",
  "packageManager": "yarn@4.0.2",
  "type": "module",
  "scripts": {
    "build": "tsc --project tsconfig.build.json",
    "dev": "yarn build --watch --noEmitOnError false",
    "test": "vitest run --coverage",
    "typecheck": "tsc --noEmit",
    "lint": "eslint",
    "clean": "rimraf dist/",
    "write-package-version-file": "yarn exec ../../write-package-version-file.sh"
  },
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./*": {
      "types": "./dist/*.d.ts",
      "import": "./dist/*.js"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "dist/*"
      ]
    }
  },
  "dependencies": {
    "@redocly/openapi-core": "^1.34.5",
    "ansi-colors": "^4.1.3",
    "commander": "^14.0.2",
    "ora": "^8.2.0"
  },
  "devDependencies": {
    "@openapi-qraft/eslint-config": "workspace:*",
    "@qraft/test-utils": "workspace:*",
    "@types/node": "^20.16.5",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^9.39.1",
    "rimraf": "^6.1.2",
    "typescript": "^5.6.2",
    "vitest": "^3.2.4"
  },
  "files": [
    "dist",
    "src",
    "!dist/**/*.test.*",
    "!dist/**/*.spec.*",
    "!src/**/__snapshots__/**",
    "!src/**/*.test.*",
    "!src/**/*.spec.*"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/OpenAPI-Qraft/openapi-qraft.git",
    "directory": "packages/plugin"
  }
}
