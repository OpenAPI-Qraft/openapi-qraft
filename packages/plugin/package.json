{
  "name": "@openapi-qraft/plugin",
  "version": "2.6.1",
  "packageManager": "yarn@4.0.2",
  "type": "module",
  "scripts": {
    "build": "tsc --project tsconfig.build.json",
    "dev": "yarn build --watch --noEmitOnError false",
    "test": "vitest run --coverage",
    "typecheck": "tsc --noEmit",
    "lint": "eslint",
    "clean": "rimraf dist/",
    "write-package-version-file": "yarn exec ../../write-package-version-file.sh"
  },
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./*": {
      "types": "./dist/*.d.ts",
      "import": "./dist/*.js"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "dist/*"
      ]
    }
  },
  "dependencies": {
    "ansi-colors": "^4.1.3",
    "camelcase": "^8.0.0",
    "commander": "^11.1.0",
    "micromatch": "^4.0.8",
    "openapi-typescript": "^7.4.0",
    "ora": "^8.1.0",
    "yaml": "^2.5.1"
  },
  "devDependencies": {
    "@openapi-qraft/eslint-config": "workspace:*",
    "@openapi-qraft/test-fixtures": "workspace:*",
    "@types/micromatch": "^4.0.9",
    "@types/node": "^20.16.5",
    "@vitest/coverage-v8": "^3.0.5",
    "commander": "^11.1.0",
    "eslint": "^9.24.0",
    "memfs": "^4.11.1",
    "rimraf": "^5.0.10",
    "typescript": "^5.6.2",
    "vitest": "^3.0.5"
  },
  "peerDependencies": {
    "@redocly/openapi-core": "^1.16.0",
    "commander": "^11.1.0"
  },
  "peerDependenciesMeta": {
    "commander": {
      "optional": false
    }
  },
  "files": [
    "dist",
    "src",
    "!dist/**/*.test.*",
    "!dist/**/*.spec.*",
    "!src/**/__snapshots__/**",
    "!src/**/*.test.*",
    "!src/**/*.spec.*"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/OpenAPI-Qraft/openapi-qraft.git",
    "directory": "packages/plugin"
  }
}
