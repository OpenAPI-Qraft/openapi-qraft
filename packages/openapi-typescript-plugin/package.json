{
  "name": "@openapi-qraft/openapi-typescript-plugin",
  "version": "1.2.0-next.8",
  "type": "module",
  "scripts": {
    "build": "tsc --project tsconfig.build.json",
    "dev": "yarn build --watch --noEmitOnError false",
    "test": "vitest run",
    "typecheck": "tsc --noEmit",
    "lint": "eslint",
    "clean": "rimraf dist/"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": "./dist/index.js",
    "./*": "./dist/*.js"
  },
  "typesVersions": {
    "*": {
      "*": [
        "dist/*"
      ]
    }
  },
  "dependencies": {
    "@openapi-qraft/plugin": "workspace:^",
    "openapi-typescript": "^7.3.0",
    "typescript": "^5.5.4"
  },
  "devDependencies": {
    "@openapi-qraft/eslint-config": "workspace:*",
    "@openapi-qraft/test-fixtures": "workspace:*",
    "@types/node": "^18.19.4",
    "eslint": "^9.0.0",
    "memfs": "^4.9.2",
    "rimraf": "^5.0.10",
    "vitest": "^1.4.0"
  },
  "files": [
    "dist",
    "src",
    "!dist/**/*.test.*",
    "!dist/**/*.spec.*",
    "!src/**/*.test.*",
    "!src/**/__snapshots__/**",
    "!src/**/*.spec.*"
  ],
  "packageManager": "yarn@4.0.2"
}
