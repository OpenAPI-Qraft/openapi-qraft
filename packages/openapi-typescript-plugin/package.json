{
  "name": "@openapi-qraft/openapi-typescript-plugin",
  "version": "1.2.0-next.17",
  "type": "module",
  "scripts": {
    "build": "tsc --project tsconfig.build.json",
    "dev": "yarn build --watch --noEmitOnError false",
    "test": "vitest run",
    "typecheck": "tsc --noEmit",
    "lint": "eslint",
    "clean": "rimraf dist/"
  },
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": "./dist/index.js",
    "./*": "./dist/*.js"
  },
  "typesVersions": {
    "*": {
      "*": [
        "dist/*"
      ]
    }
  },
  "dependencies": {
    "@openapi-qraft/plugin": "workspace:^",
    "openapi-typescript": "^7.4.0",
    "typescript": "^5.6.2"
  },
  "devDependencies": {
    "@openapi-qraft/eslint-config": "workspace:*",
    "@openapi-qraft/test-fixtures": "workspace:*",
    "@types/node": "^20.16.5",
    "eslint": "^9.16.0",
    "memfs": "^4.11.1",
    "rimraf": "^5.0.10",
    "vitest": "^2.1.8"
  },
  "files": [
    "dist",
    "src",
    "!dist/**/*.test.*",
    "!dist/**/*.spec.*",
    "!src/**/*.test.*",
    "!src/**/__snapshots__/**",
    "!src/**/*.spec.*"
  ],
  "packageManager": "yarn@4.0.2"
}
