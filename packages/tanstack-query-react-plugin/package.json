{
  "name": "@openapi-qraft/tanstack-query-react-plugin",
  "version": "2.15.0-beta.3",
  "type": "module",
  "scripts": {
    "build": "tsc --project tsconfig.build.json",
    "codegen": "yarn exec ./generate-ts-factory.mjs",
    "dev": "yarn build --watch --noEmitOnError false",
    "test": "vitest run",
    "typecheck": "tsc --noEmit",
    "lint": "eslint",
    "clean": "rimraf dist/"
  },
  "publishConfig": {
    "access": "public"
  },
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./*": {
      "types": "./dist/*.d.ts",
      "import": "./dist/*.js"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "dist/*"
      ]
    }
  },
  "dependencies": {
    "@openapi-qraft/plugin": "workspace:^",
    "@qraft/cli-utils": "workspace:*",
    "@qraft/plugin": "workspace:^",
    "ansi-colors": "^4.1.3",
    "camelcase": "^8.0.0",
    "commander": "^14.0.2",
    "ora": "^8.2.0",
    "typescript": "^5.6.2"
  },
  "devDependencies": {
    "@openapi-qraft/eslint-config": "workspace:*",
    "@openapi-qraft/tanstack-query-react-types": "workspace:*",
    "@openapi-qraft/test-fixtures": "workspace:*",
    "@openapi-qraft/ts-factory-code-generator": "workspace:*",
    "@qraft/test-utils": "workspace:*",
    "camelcase": "^8.0.0",
    "eslint": "^9.39.1",
    "memfs": "^4.56.10",
    "rimraf": "^6.1.2",
    "vitest": "^3.2.4"
  },
  "files": [
    "dist",
    "src",
    "!dist/**/*.test.*",
    "!dist/**/*.spec.*",
    "!src/**/__snapshots__/**",
    "!src/**/*.test.*",
    "!src/**/*.spec.*"
  ],
  "packageManager": "yarn@4.0.2",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/OpenAPI-Qraft/openapi-qraft.git",
    "directory": "packages/cli"
  }
}
