{
  "name": "@openapi-qraft/tanstack-query-react-plugin",
  "version": "2.2.0",
  "type": "module",
  "scripts": {
    "build": "tsc --project tsconfig.build.json",
    "codegen": "yarn exec ./generate-ts-factory.mjs",
    "dev": "yarn build --watch --noEmitOnError false",
    "test": "vitest run",
    "typecheck": "tsc --noEmit",
    "lint": "eslint",
    "clean": "rimraf dist/"
  },
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": "./dist/index.js",
    "./*": "./dist/*.js"
  },
  "typesVersions": {
    "*": {
      "*": [
        "dist/*"
      ]
    }
  },
  "dependencies": {
    "@openapi-qraft/plugin": "workspace:^",
    "typescript": "^5.6.2"
  },
  "devDependencies": {
    "@openapi-qraft/eslint-config": "workspace:*",
    "@openapi-qraft/test-fixtures": "workspace:*",
    "@openapi-qraft/ts-factory-code-generator": "workspace:*",
    "camelcase": "^8.0.0",
    "eslint": "^9.20.1",
    "memfs": "^4.11.1",
    "rimraf": "^5.0.10",
    "vitest": "^3.0.5"
  },
  "peerDependencies": {
    "commander": "^11.1.0"
  },
  "peerDependenciesMeta": {
    "commander": {
      "optional": false
    }
  },
  "files": [
    "dist",
    "src",
    "!dist/**/*.test.*",
    "!dist/**/*.spec.*",
    "!src/**/__snapshots__/**",
    "!src/**/*.test.*",
    "!src/**/*.spec.*"
  ],
  "packageManager": "yarn@4.0.2"
}
