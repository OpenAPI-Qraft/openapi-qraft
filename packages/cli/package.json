{
  "name": "@openapi-qraft/cli",
  "version": "1.11.0-beta.0",
  "description": "CLI for generating typed Tanstack Query React Hooks and services from OpenAPI Schemas, improving type safety in React apps",
  "scripts": {
    "build": "rimraf dist/ && tsc --project tsconfig.build.json",
    "dev": "yarn build --watch",
    "test": "vitest run",
    "typecheck": "tsc --noEmit",
    "lint": "eslint"
  },
  "type": "module",
  "bin": {
    "openapi-qraft": "./bin.mjs"
  },
  "dependencies": {
    "@openapi-qraft/openapi-typescript-plugin": "workspace:~",
    "@openapi-qraft/plugin": "workspace:~",
    "@openapi-qraft/tanstack-query-react-plugin": "workspace:~",
    "commander": "^11.1.0"
  },
  "devDependencies": {
    "@openapi-qraft/eslint-config": "workspace:~",
    "@types/node": "^18.19.4",
    "eslint": "^9.0.0",
    "globals": "^15.0.0",
    "rimraf": "^5.0.5",
    "typescript": "^5.3.3",
    "vitest": "^1.4.0"
  },
  "files": [
    "dist",
    "src",
    "!dist/**/*.test.*",
    "!dist/**/*.spec.*",
    "!src/**/*.test.*",
    "!src/**/*.spec.*"
  ],
  "exports": {
    "./package.json": "./package.json",
    "./*": "./dist/*.js"
  },
  "typesVersions": {
    "*": {
      "*": [
        "dist/*"
      ]
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/OpenAPI-Qraft/openapi-qraft.git",
    "directory": "packages/cli"
  },
  "bugs": {
    "url": "https://github.com/OpenAPI-Qraft/openapi-qraft/issues"
  },
  "homepage": "https://openapi-qraft.github.io/openapi-qraft/",
  "keywords": [
    "openapi",
    "swagger",
    "rest",
    "api",
    "oapi_3",
    "oapi_3_1",
    "typescript",
    "ts",
    "dts",
    "codegen",
    "generation",
    "fetch",
    "react",
    "hooks",
    "TanStack Query"
  ],
  "publishConfig": {
    "access": "public"
  }
}
