{
  "name": "@openapi-qraft/cli",
  "version": "1.6.0",
  "description": "CLI for generating typed Tanstack Query React Hooks and services from OpenAPI Schemas, improving type safety in React apps",
  "scripts": {
    "build": "rimraf dist/ && tsc --project tsconfig.build.json",
    "dev": "yarn build --watch",
    "test": "vitest run",
    "typecheck": "tsc --noEmit"
  },
  "type": "module",
  "bin": {
    "openapi-qraft": "./bin.mjs"
  },
  "dependencies": {
    "ansi-colors": "^4.1.3",
    "camelcase": "^8.0.0",
    "commander": "^11.1.0",
    "micromatch": "^4.0.5",
    "openapi-typescript": "7.0.0-next.7",
    "ora": "^8.0.1",
    "typescript": "^5.3.3",
    "yaml": "^2.3.4"
  },
  "devDependencies": {
    "@types/micromatch": "^4",
    "@types/node": "^18.19.4",
    "rimraf": "^5.0.5",
    "vitest": "^1.4.0"
  },
  "files": [
    "dist",
    "bin.mjs"
  ],
  "exports": {
    "./package.json": "./package.json",
    "./*": "./dist/*.js"
  },
  "typesVersions": {
    "*": {
      "*": [
        "dist/*"
      ]
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/OpenAPI-Qraft/openapi-qraft.git",
    "directory": "packages/cli"
  },
  "bugs": {
    "url": "https://github.com/OpenAPI-Qraft/openapi-qraft/issues"
  },
  "homepage": "https://openapi-qraft.github.io/openapi-qraft/",
  "keywords": [
    "openapi",
    "swagger",
    "rest",
    "api",
    "oapi_3",
    "oapi_3_1",
    "typescript",
    "ts",
    "dts",
    "codegen",
    "generation",
    "fetch",
    "react",
    "hooks",
    "TanStack Query"
  ],
  "publishConfig": {
    "access": "public"
  }
}
